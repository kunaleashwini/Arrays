
class t {
    public static void main(String[] args) {
        int arr[] = { 1, 3, 2, 7, 11, 8, 5 };
        int n = arr.length;

        int temp;
        for (int i = 0; i < n; i = i + 2) {
            if ((i + 1) < n) {
                temp = arr[i];
                arr[i] = arr[i + 1];
                arr[i + 1] = temp;
            }
        }

        for (int i = 0; i < n; i++) {
            System.out.print(arr[i] + " ");
        }

    }
}
